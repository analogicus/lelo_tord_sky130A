* Measure dac
.control

load {cicname}.raw

let v(b0_n) = v(xdut.b0_n) 
let v(b1_n) = v(xdut.b1_n) 
let v(b2_n) = v(xdut.b2_n) 
let v(b3_n) = v(xdut.b3_n)
let v(b4_n) = v(xdut.b4_n)
let v(pbias) = v(xdut.pbias)
let v(nbias) = v(xdut.nbias)
let v(vdiode) = v(xdut.vdiode)

echo "MEAS_START"

set wr_singlescale
set wr_vecnames
option numdgt = 3
wrdata {cicname}.out 
+ v(dec_b)
+ v(b0) v(b1) v(b2) v(b3) v(b4)
+ v(b0_n) v(b1_n) v(b2_n) v(b3_n) v(b4_n)
+ v(vdiode)
+ v(CLK) v(RST) 
+ v(PBIAS) v(NBIAS) v(SLEEP)
+ v(VSS) v(VDD)

echo "MEAS_END"
.endc
