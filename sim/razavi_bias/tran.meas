* Measure razavi_bias
.control

load {cicname}.raw

echo "MEAS_START"

meas tran iv1 find i(v.xdut.v1) at=20n
meas tran iv2 find i(v.xdut.v2) at=20n

meas tran vg1 find v(xdut.vg1) at=20n
meas tran vg2 find v(xdut.vg2) at=20n

meas tran v1 find v(xdut.v1) at=20n
meas tran v2 find v(xdut.v2) at=20n
meas tran vr find v(xdut.vr) at=20n

meas tran ivdd find i(vdd) at=20n
meas tran ivss find i(vss) at=20n

meas tran vdd find v(vdd) at=20n
meas tran vss find v(vss) at=20n

echo "MEAS_END"
.endc
