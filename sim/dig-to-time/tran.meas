* Measure dig-to-time
.control

load {cicname}.raw

echo "MEAS_START"

set wr_singlescale
set wr_vecnames
option numdgt = 3
wrdata {cicname}.out 
+ v(d0) v(d1) v(d2) v(d3)
+ v(CLK) v(RST) 
+ v(VSS) v(VDD)
+ v(xdut.A) v(xdut.B) v(xdut.C) v(xdut.D)
+ v(xdut.D0andA) v(xdut.D1andB) v(xdut.D2andC) v(xdut.D3andD)
+ v(xdut.AorB) v(xdut.CorD) 
+ v(xdut.R) v(xdut.R_n)
+ v(xdut.OUT) v(Q) 
+ v(xdut.D0orD1) v(xdut.D2orD3) v(xdut.EN)
* + v(Q_N) v(xdut.R)
* + v(dec_d)
* + v(dec_count)

echo "MEAS_END"
.endc
